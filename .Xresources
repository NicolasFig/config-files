! Use xrdb to apply changes
! special
*.foreground:   #a6a28c
*.background:   #20201d
*.cursorColor:  #a6a28c
*utf8: font

! 1
xterm*faceName: Liberation Sans:size=11:antialias=true
URxvt.font: xft:Source Code Pro:size=11:antialias=true

! scrollbar
URxvt.scrollBar: false

! black
*.color0:       #20201d
*.color8:       #7d7a68

! red
*.color1:       #d73737
*.color9:       #d73737

! green
*.color2:       #60ac39
*.color10:      #60ac39

! yellow
*.color3:       #cfb017
*.color11:      #cfb017

! blue
*.color4:       #6684e1
*.color12:      #6684e1

! magenta
*.color5:       #b854d4
*.color13:      #b854d4

! cyan
*.color6:       #1fad83
*.color14:      #1fad83

! white
*.color7:       #a6a28c
*.color15:      #fefbec

! padding
*.internalBorder: 10

! border
*borderWidth: 0


